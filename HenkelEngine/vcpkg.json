{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
  "name": "gameengine",
  "version": "1",
  "dependencies": [
    "glew",
    "sdl2",
    "soil2",
    "glm",
    "box2d",
    "zlib",
    "sol2",
    "entt",
    "nlohmann-json",
    "pugixml",
    "lua",
    {
      "name": "imgui",
      "features": [
        "sdl2-binding",
        "opengl3-binding",
        "docking-experimental"
      ],
      "platform": "x64"
    }
  ]
}
